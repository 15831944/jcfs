					<!--Option name="filter_phone" group="extension_filter" height="50" width="80" selectedimage="type\sj_b.png" normalimage="type\sj_a.png" userdata=""></Option-->
					<Option name="filter_zip" group="extension_filter" height="50" width="80" selectedimage="type\ys_b.png" normalimage="type\ys_a.png" userdata="7z;ace;arj;bz2;cab;gz;gzip;r00;r01;r02;r03;r04;r05;r06;r07;r08;r09;r10;r11;r12;r13;r14;r15;r16;r17;r18;r19;r20;r21;r22;r23;r24;r25;r26;r27;r28;r29;rar;tar;tgz;z;zip" ></Option>
