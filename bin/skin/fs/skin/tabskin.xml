					<Control width="5"></Control>
					<Option name="option_match_case" selected="false" width="24" height="24" tooltip="区分大小写" selectedimage="button\dxx_c.png" normalimage="button\dxx_a.png" pushedimage="button\dxx_b.png"></Option>
					<Control width="5"></Control>
					<Option name="option_match_exclude_system" selected="false" width="24" height="24" tooltip="排除系统文件（目录）" selectedimage="button\xt_c.png" normalimage="button\xt_a.png" pushedimage="button\xt_b.png"></Option>
					<Control width="5"></Control>
					<Option name="option_match_exclude_hidden" selected="false" width="24" height="24" tooltip="排除隐藏文件（目录）" selectedimage="button\yc_c.png" normalimage="button\yc_a.png" pushedimage="button\yc_b.png"></Option>
