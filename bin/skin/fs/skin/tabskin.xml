				<VerticalLayout >
					<HorizontalLayout bkimage="file='edit\srk.png' corner='4,1,4,1'" inset="1,1,1,1" height="24" >
						<Edit name="search_input_edit" textcolor="#FF00ff00"></Edit>
						<VerticalLayout width="14">
							<Control height="4"></Control>
							<Control height="14" width="14" bkimage="edit\ss.png"></Control>
						</VerticalLayout>
					</HorizontalLayout>
				</VerticalLayout>


				<HorizontalLayout width="121">
