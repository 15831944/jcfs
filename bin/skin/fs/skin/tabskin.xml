							<Combo height="1" name="path_filter" normalimage="file='edit\srk.png' corner='4,1,4,1'" hotimage="file='edit\srk2.png' corner='4,1,4,1'" itemaltbk="true" hscrollbar="true" itemfont="0" dropboxsize="0,250" itemendellipsis="true" multiexpanding="true" selclose="false" tooltip="请点击这里选择搜索历史记录">

								<HorizontalLayout  inset="10,5,0,5" height="24">
									<Button name="path_select_btn" bkimage="text\tjjl.png" width="91"></Button>
								</HorizontalLayout>
							</Combo>

						</VerticalLayout>

						<Control width="2"></Control>
						<Option name="option_filter_drop_down" selected="false" width="24" height="24"  selectedimage="button\xl_c.png" normalimage="button\xl_a.png" pushedimage="button\xl_b.png" ></Option>
						<Control width="8"></Control>

						<VerticalLayout width="135">
							<HorizontalLayout>
								<VerticalLayout width="84">
									<Control height="6"></Control>
